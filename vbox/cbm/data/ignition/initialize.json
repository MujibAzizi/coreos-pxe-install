{
  "ignitionVersion": 1,
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa  spencer@Spencers-MacBook-Pro-2.local"
        ]
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "name": "set-hostname.service",
        "enable": true,
        "contents": "[Unit]\n[Service]\nType=oneshot\nExecStart=/usr/bin/hostnamectl set-hostname vbox-01\n[Install]\nWantedBy=multi-user.target"
      }
    ]
  },
  "storage": {
    "disks": [
      {
        "device": "/dev/sda",
        "wipeTable": true,
        "partitions": [
          {
            "label": "ROOT",
            "number": 0
          }
        ]
      }
    ],
    "filesystems": [
      {
        "device": "/dev/sda1",
        "format": "ext4",
        "create": {
          "force": true,
          "options": [
            "-i 8192"
          ]
        }
      }
    ]
  }
}